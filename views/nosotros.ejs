<%- include('partials/header', { titulo: 'Nosotros' }) %>
<main id="main-content">
  <% if (secciones && secciones.length) { %>
    <% secciones.forEach(function(seccion) { %>
      <section class="seccion-<%= seccion.seccion %>">
        <% if (seccion.titulo) { %>
          <h2><%= seccion.titulo %></h2>
        <% } %>
        <%- seccion.contenido %>
      </section>
    <% }) %>
  <% } else { %>
    <p>No hay informaci√≥n disponible.</p>
  <% } %>
</main>
<%- include('partials/footer') %>